{
  "key": "cmd+l",
  "command": ["HookyQR.beautify"],
  "when": "editorFocus",
  "indent_size": 4,
  "indent_char": " ",
  "css": {
    "indent_size": 2,
    "end_with_newline": true,
    "preserve-newlines": true
  },
  "js":{
    "end_with_newline": true
  },
  "eol": "\n",
  "operator_position": "before-newline"
}